{
  "coc.preferences.semanticTokensHighlights": false,
  "prettier.trailingComma": "es5",
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "typescript.format.semicolons": "remove",
  "explorer.icon.enableNerdfont": true,
  "explorer.file.column.indent.indentLine": true,
  "explorer.floating.border.chars": ["─", "│", "─", "│", "╭", "╮", "╯", "╰"],
  "diagnostic-languageserver.filetypes": {
    "zsh": "shellcheck",
    "sh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "zsh": "shfmt",
    "sh": "shfmt"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt",
      "args": ["-i", "2", "-bn", "-ci", "-sr"]
    }
  },
  "yaml.format.enable": true,
  "yaml.format.singleQuote": true,
  "html.autoClosingTags": true,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "flutter.workspaceFolder.ignore": [
    "~/.pub-cache",
    "~/fvm"
  ],
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "cSpell.enabledLanguageIds": [
    "vim",
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml",
    "dart"
  ],
  "cSpell.userWords": ["Jetpack", "Mergeable", "frontmatter", "octokit"]
}
